{
  "name": "cloudflare-images",
  "version": "1.0.0",
  "description": "Cloudflare Workers implementation of Next.js Image optimization",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "dev:local": "wrangler dev src/index.ts --local",
    "deploy": "wrangler deploy --minify",
    "deploy:dry": "wrangler deploy --dry-run",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "echo \"No linter configured\" && exit 0",
    "format": "echo \"No formatter configured\" && exit 0",
    "preview": "wrangler dev --local-protocol=https",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["cloudflare", "workers", "next.js", "image-optimization"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "@hono/zod-validator": "^0.7.0",
    "hono": "^4.8.4",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250705.0",
    "@types/node": "^24.0.10",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.23.0"
  }
}
